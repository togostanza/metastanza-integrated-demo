{
  "dataTypes": {
    "matrix": {
      "label": "Matrix Data",
      "description": "Multi-dimensional data visualization with charts and tables",
      "dataSource": {
        "url": "./data/matrix-data.json",
        "receiver": "togostanza-pagination-table, togostanza-heatmap, togostanza-piechart, togostanza-barchart, togostanza-linechart, togostanza-scatter-plot, togostanza-histogram"
      },
      "stanzas": [
        {
          "title": "Pie chart",
          "id": "piechart",
          "attributes": {
            "data-url": "",
            "data-type": "json",
            "data-value_key": "area",
            "data-category_key": "prefecture",
            "color-key": "",
            "group-key": "",
            "legend-title": "Population",
            "tooltip": "{{prefecture}}",
            "togostanza-menu_placement": "none",
            "event-outgoing_change_selected_nodes": "",
            "event-incoming_change_selected_nodes": ""
          },
          "cssVariables": {
            "--togostanza-canvas-width": 200,
            "--togostanza-canvas-height": 200
          }
        },
        {
          "title": "Scatter plot",
          "id": "scatter-plot",
          "attributes": {
            "data-url": "",
            "data-type": "json",
            "axis-x-key": "pop_2020",
            "axis-x-title": "人口",
            "axis-x-title_padding": 20,
            "axis-x-ticks_label_angle": -45,
            "axis-x-scale": "linear",
            "axis-x-ticks_interval": "",
            "axis-x-ticks_interval_units": "none",
            "axis-x-ticks_labels_format": ",.2r",
            "axis-x-gridlines_interval": "",
            "axis-x-gridlines_interval_units": "none",
            "axis-y-key": "income_2020",
            "axis-y-title": "収入",
            "axis-y-title_padding": 40,
            "axis-y-ticks_label_angle": 0,
            "axis-y-scale": "linear",
            "axis-y-ticks_interval": "",
            "axis-y-ticks_labels_format": ",.2r",
            "axis-y-gridlines_interval": "",
            "node-url_key": "url",
            "node-size_key": "area",
            "node-size_min": 3,
            "node-size_max": 10,
            "tooltip": "{{prefecture}}",
            "togostanza-menu_placement": "none",
            "event-outgoing_change_selected_nodes": "",
            "event-incoming_change_selected_nodes": ""
          },
          "cssVariables": {
            "--togostanza-canvas-width": 300,
            "--togostanza-canvas-height": 200
          }
        },
        {
          "title": "Bar chart",
          "id": "barchart",
